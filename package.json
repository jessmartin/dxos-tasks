{
  "name": "dxos-tasks",
  "version": "0.1.0",
  "description": "dxos-tasks - a DXOS application",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit && NODE_OPTIONS=\"--max-old-space-size=4096\" vite build",
    "deploy": "NODE_OPTIONS=\"--max-old-space-size=4096\" dx app publish",
    "preview": "vite preview",
    "prebuild": "dxtype src/proto/schema.proto src/proto/gen/schema.ts",
    "serve": "npm run prebuild && vite",
    "storybook": "start-storybook -p 9009 --no-open"
  },
  "dependencies": {
    "@dxos/aurora": "0.1.51",
    "@dxos/client": "0.1.51",
    "@dxos/config": "0.1.51",
    "@dxos/react-appkit": "0.1.51",
    "@dxos/react-client": "0.1.51",
    "@dxos/react-shell": "^0.1.51",
    "@dxos/vault": "0.1.51",
    "@phosphor-icons/react": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@dxos/aurora-theme": "0.1.51",
    "@dxos/cli": "0.1.51",
    "@dxos/echo-schema": "^0.1.51",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/builder-vite": "^7.0.6",
    "@storybook/core-server": "^7.0.6",
    "@storybook/mdx2-csf": "^1.0.0",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.21",
    "tailwindcss": "~3.2.7",
    "typescript": "^4.8.4",
    "vite": "^4.3.0",
    "vite-plugin-pwa": "^0.14.1",
    "workbox-window": "^6.5.4"
  }
}
